{
  "name": "google-calendar-subscriptions-sync",
  "version": "0.0.1",
  "homepage": "https://github.com/gabrielecanepa/google-calendar-subscriptions-sync",
  "repository": {
    "type": "git",
    "url": "https://github.com/gabrielecanepa/google-calendar-subscriptions-sync.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@8.10.5",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "scripts": {
    "prepare": "husky install",
    "build": "rm -rf build && webpack --mode production",
    "start": "node build/index.js",
    "dev": "ts-node src/index.ts",
    "sync": "pnpm build >/dev/null && pnpm start",
    "clear": "ts-node scripts/clear.ts",
    "new": "ts-node scripts/new.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "pnpm lint --fix",
    "ci": "pnpm lint"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.54.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "husky": "^8.0.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "google-calendar-subscriptions": "^1.1.0"
  }
}
