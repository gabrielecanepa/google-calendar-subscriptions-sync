{
  "name": "google-calendar-subscriptions-sync",
  "version": "0.0.1",
  "homepage": "https://github.com/gabrielecanepa/google-calendar-subscriptions-sync",
  "repository": {
    "type": "git",
    "url": "https://github.com/gabrielecanepa/google-calendar-subscriptions-sync.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@8.10.5",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "scripts": {
    "prepare": "husky install",
    "build": "rm -rf build && webpack --mode production",
    "action": "pnpm build >/dev/null && node build/index.js",
    "action:dev": "ts-node src/index.ts",
    "sync": "pnpm action sync",
    "sync:dev": "pnpm action:dev sync",
    "clear": "pnpm action clear",
    "clear:dev": "pnpm action:dev clear",
    "insert": "pnpm action insert",
    "insert:dev": "pnpm action:dev insert",
    "lint": "eslint . --ext .ts",
    "lint:fix": "pnpm lint --fix",
    "ci": "pnpm lint"
  },
  "dependencies": {
    "@msgpack/msgpack": "2.8.0",
    "google-calendar-subscriptions": "^1.2.0",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.19.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "prettier": "^3.1.0",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
